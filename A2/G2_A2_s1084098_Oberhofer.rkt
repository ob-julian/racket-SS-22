#|We try to make our own if.
Now we replace the built-in if with myif in the Newton-example discussed in the lecture. What
happens? Explain (use Racket comments for the explanation).|#

(define (myif predicate then-clause else-clause)
 (cond (predicate then-clause)
 (else else-clause)))

(define (sqrt-iter-myif guess x)
  (define (good-enough? guess)
    (< (abs (- (* guess guess) x)) 0.000001))
  (myif (good-enough? guess)
      guess
      (sqrt-iter-myif (/ (+ guess (/ x guess)) 2) x)))

(define (sqrt-myif x)
  (define (square guess) (* guess guess))
  (define (average guess y) (/ (+ guess y) 2))
  (define (good-enough? guess)
    (< (abs (- (square guess) x)) 0.000001))
  (define (improve guess)
    (average guess (/ x guess)))
  (define (sqrt-iter guess)
    (if (good-enough? guess)
          guess
          (sqrt-iter (improve guess))))
  (sqrt-iter 1.0))


