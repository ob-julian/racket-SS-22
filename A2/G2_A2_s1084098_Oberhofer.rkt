(define (powerCloseTo b n [e 1])
  (if (and (> b 1) (> n 0) (<= (expt b e) n))
      (powerCloseTo b n (add1 e))
      e)) 

(define (powerCloseTo2 b n)
  (define (powerCloseTo-itero e)
        (if (> (expt b e) n)
            e
            (powerCloseTo-itero (add1 e))))
  (if (and (> b 0) (> n 0))                      
      (powerCloseTo-itero 1)
      #f)) 

